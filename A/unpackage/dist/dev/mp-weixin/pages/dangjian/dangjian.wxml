<view><view class="swiperBox"><swiper class="swiperBox" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><swiper-item><view class="swiper-item"><image class="banner" src="http://www.ynftc.cn/upload/main/advertisement/b7dbc382c29348eabf98e934b303c4dc_1920_500.jpg"></image></view></swiper-item><swiper-item><view class="swiper-item"><image class="banner" src="http://www.ynftc.cn/upload/main/advertisement/bbae7f12c2594d0092c2983c5179b1b1_1920_500.jpg"></image></view></swiper-item><swiper-item><view class="swiper-item"><image class="banner" src="http://www.ynftc.cn/upload/main/advertisement/337d889a5c2040b097a2d17539a90f5b_1920_500.jpg"></image></view></swiper-item></swiper></view><view><view class="serviceBox"><block wx:for="{{service}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['toService',[index]]]]]}}" class="service-top-item" bindtap="__e"><image class="service-top-item-icon" src="{{item.img}}"></image><view class="service-top-item-icon-desc">{{item.name}}</view></view></block></block></view></view><view><view class="newbox"><scroll-view class="scroll-x" scroll-x="true"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:if="{{item.id!=type}}"><text data-event-opts="{{[['tap',[['choose',['$0'],[[['tabList','',index,'id']]]]]]]}}" class="title" bindtap="__e">{{item.name}}</text></block><block wx:else><text data-event-opts="{{[['tap',[['choose',['$0'],[[['tabList','',index,'id']]]]]]]}}" class="title itemTabSelect" bindtap="__e">{{item.name}}</text></block></block></block></scroll-view><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="id"><block><view data-event-opts="{{[['tap',[['getNewItem',['$0'],[[['newDataList','id',item.$orig.id,'id']]]]]]]}}" class="bg" bindtap="__e"><view class="newtitle">{{item.$orig.title+''}}<image class="newimg" src="{{item.m0}}"></image></view><view><rich-text class="desc" nodes="{{item.$orig.content.length>40?item.g0+'……':item.$orig.content}}"></rich-text></view><view><text class="time">{{"评论总数："+(item.$orig.commentNum>0?item.$orig.commentNum:0)}}</text></view><view><text class="time">{{"发布时间："+item.$orig.publishDate}}</text></view></view></block></block></view></view></view>